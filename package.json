{
  "name": "Epiviz",
  "version": "0.0.1",
  "description": "Genomics Data Visualization Platform",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "build --dir",
    "dist": "build"
  },
  "build": {
    "appId": "org.epiviz.epiviz",
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "extraResources": [
      "epiviz.sh"
    ]
  },
  "repository": "https://github.com/jkanche/epiviz-electron",
  "keywords": [
    "epiviz",
    "genomics",
    "data",
    "visualization"
  ],
  "postinstall": "install-app-deps",
  "author": "Jayaram Kancherla <jkanche@umiacs.umd.edu>",
  "license": "CC0-1.0",
  "devDependencies": {
    "ajv-keywords": "^1.5.1",
    "electron": "^9.1.0",
    "electron-builder": "^15.6.4",
    "electron-packager": "^8.6.0",
    "hdiutil": "0.0.6"
  },
  "dependencies": {
    "yargs": "^7.0.2"
  }
}
